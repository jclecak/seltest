			</parameters>
			<classes>
				<class name="SeGridTest" />
			</classes>
		</test>
		<test name="Mac+Firefox Test">
			<parameters>
				<parameter name="platform" value="Mac" />
				<parameter name="browser" value="Firefox" />
				<parameter name="version" value="38.0.5" />
				<parameter name="url" value="file:///home/jclecak/selbook/bmicalc.html" />
			</parameters>
			<classes>
				<class name="SeGridTest" />
			</classes>
		</test>
		<test name="Mac+Safari Test">
			<parameters>
				<parameter name="platform" value="Mac" />
				<parameter name="browser" value="Safari" />
				<parameter name="version" value="10.1" />
				<parameter name="url" value="file:///home/jclecak/selbook/bmicalc.html" />
			</parameters>
			<classes>
				<class name="SeGridTest" />
			</classes>
		</test>
	</tests>
</suite>