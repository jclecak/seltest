<suite name="Parallel Tests" verbose="1" thread-count="3"
	parallel="tests">
	<tests>
		<test name="Linux+Chrome">
			<parameters>
				<parameter name="platform" value="Linux" />
				<parameter name="browser" value="Chrome" />
				<parameter name="version" value="35" />
				<parameter name="url" value="file:///home/jclecak/selbook/bmicalc.html" />
			</parameters>
			<classes>
				<class name="SeGridTest" />
			</classes>
		</test>
		<test name="Windows+IE10 Test">
			<parameters>
				<parameter name="platform" value="Windows" />
				<parameter name="browser" value="Internet Explorer" />
